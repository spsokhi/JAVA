package WhileLoop;

import java.util.Scanner;

public class Palendrome {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		
		int num=sc.nextInt();
		int b=num;
		int rev=0;
		while(num!=0)
		{
			int temp= num%10;
			 rev=rev*10+temp;
			num/=10;
	    }
		if(b==rev)
		{
			System.out.println("palendrome");
		}
		else
		{
			System.out.println("not palendrome");
		}
	}
}
